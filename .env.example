# swiftETH Environment Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# RPC Configuration
RPC_URL=https://eth.llamarpc.com
RPC_URL_TESTNET=https://sepolia.infura.io/v3/YOUR_INFURA_KEY

# Test Account (for development/testing only)
# NEVER use accounts with real funds
# Use testnet accounts only
TEST_PRIVATE_KEY=

# Optional: Alternative RPC Providers
# Infura
INFURA_API_KEY=
INFURA_MAINNET=https://mainnet.infura.io/v3/YOUR_INFURA_KEY
INFURA_SEPOLIA=https://sepolia.infura.io/v3/YOUR_INFURA_KEY

# Alchemy
ALCHEMY_API_KEY=
ALCHEMY_MAINNET=https://eth-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
ALCHEMY_SEPOLIA=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# QuickNode
QUICKNODE_MAINNET=
QUICKNODE_SEPOLIA=

# Network Configuration
CHAIN_ID=1
NETWORK_NAME=mainnet

# Gas Configuration (optional overrides)
DEFAULT_GAS_LIMIT=21000
GAS_PRICE_MULTIPLIER=1.0

# Application Settings
LOG_LEVEL=info
DEBUG_MODE=false
